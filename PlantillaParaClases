package nombre_paquete;

public class Calculadora { //Cambia el nombre de la clase según sea necesario
    public static void main(String[] args) {
        // Prueba del método sumar
        int resultado = sumar(5, 3);
        System.out.println("Resultado: " + resultado); // Debería mostrar 8
    }

    public static int sumar(int a, int b) { 
        /*
         * Cambia el nombre del método y su funcionalidad según sea necesario
         */
        // Retorna la suma de dos números enteros
        return a + b;
    }
}
